<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @track.name %>
</p>

<p>
  <strong>Album:</strong>
  <%= @track.album_id %>
</p>

<p>
  <strong>Ord:</strong>
  <%= @track.ord %>
</p>

<p>
  <strong>Bonus:</strong>
  <%= @track.bonus %>
</p>

<p>
  <strong>Lyrics:</strong>
  <%= @track.lyrics %>
</p>

<h4 class='sub-header'>Links</h4>
<ul class='page-links'>
  <li><%= link_to 'Edit Track', edit_track_path(@track) %></li>
  <li>
    <form action="<%= track_url(@track) %>" method="POST">
      <input type="hidden" name="_method" value="DELETE">
      <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
      <input type="submit" value="Destroy Track">
    </form>
  </li>
  <li><%= link_to 'Back to Album', album_url(@track.album) %></li>
</ul>
